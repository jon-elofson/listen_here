<html>
<head>
<link href='//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css' rel='stylesheet'>
<link href="../css/style.css" rel="stylesheet" type="text/css">

</head>
<body>

<%- include header.html.ejs %>
<div id="map"></div>
   <script type="text/javascript">

   var styles = [
       {
         "featureType": "landscape",
         "stylers": [
           { "color": "#808080" }
         ]
       },{
         "featureType": "water",
         "stylers": [
           { "color": "#000000" }
         ]
       },{
         "featureType": "road",
         "stylers": [
           { "color": "#d64505" },
           { "visibility": "simplified" }
         ]
       },{
         "elementType": "labels.icon",
         "stylers": [
           { "visibility": "off" }
         ]
       },{
         "featureType": "landscape.natural.landcover",
         "stylers": [
           { "color": "#192980" },
           { "visibility": "off" }
         ]
       },{
         "featureType": "landscape.natural.terrain",
         "elementType": "geometry.fill",
         "stylers": [
           { "visibility": "on" },
           { "color": "#2f2380" }
         ]
       },{
         "featureType": "poi",
         "stylers": [
           { "visibility": "off" }
         ]
       },{
         "featureType": "road.highway",
         "stylers": [
           { "color": "#ee5858" }
         ]
       },{
         "featureType": "water",
         "stylers": [
           { "visibility": "on" },
           { "color": "#29293e" }
         ]
       },{
         "featureType": "landscape",
         "stylers": [
           { "visibility": "on" },
           { "color": "#000000" }
         ]
       },
       {
         "featureType": "administrative.province",
         "elementType": "labels",
         "stylers": [
      { "visibility": "on" },
      { "weight": 0.5 },
      { "color": "#ffffff" }
    ]
  },
  {
   "featureType": "administrative.country",
   "elementType": "labels",
   "stylers": [
     { "weight": 0.5 },
     { "color": "#ffffff" },
     { "visibility": "off" }
   ]
 }
   ];
var map;
function initMap() {
 map = new google.maps.Map(document.getElementById('map'), {
   center: {lat: 38.8833, lng: -100.0167},
   zoom: 5
 });
 map.setOptions({styles: styles});
}



   </script>
   <script async defer
     src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAYNPQ3d-yaaTqzlK4qC8ovLPD0Pf6ypz8&callback=initMap">
   </script>

</body>
</html>
